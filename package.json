{
  "name": "@winkgroup/misc",
  "version": "2.0.10",
  "description": "Utility library used in WiNK for miscellaneous tasks",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "clean": "rm -rf dist/*",

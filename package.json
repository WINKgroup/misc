  "bugs": {
    "url": "https://github.com/WINKgroup/misc-wink/issues"
  },
  "homepage": "https://github.com/WINKgroup/misc-wink#readme",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.30",
    "prettier": "^3.2.5",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0"
  }
}
